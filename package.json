{
  "name": "@o2xp/react-datatable",
  "description": "@o2xp/react-datatable is a kanban with smooth drag and drop",
  "keywords": [
    "react",
    "component",
    "kanban",
    "drag-and-drop",
    "dnd",
    "drag"
  ],
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/nicolasDdn/datatable"
  },
  "version": "1.0.0",
  "dependencies": {
    "@material-ui/core": "^4.10.1",
    "@material-ui/icons": "^4.5.1",
    "clsx": "^1.1.1",
    "moment": "^2.29.1",
    "react": "^16.8.6",
    "react-scripts": "^3.4.3",
    "react-scroll-sync": "^0.8.0",
    "react-sortable-hoc": "^1.11.0",
    "react-window": "^1.8.5",
    "uuid": "^8.3.1"
  },
  "scripts": {
    "flow": "flow",
    "start": "react-scripts start",
    "build": "webpack --mode production",
    "test": "react-scripts test --coverage --watchAll=false --silent",
    "testDev": "react-scripts test --coverage --watchAll=false",
    "eject": "react-scripts eject",
    "lint": "eslint ./src/**/*.{js,jsx} --fix",
    "prettier": "prettier --write ./src/**/*.{js,jsx}",
    "version": "echo $npm_package_version",
    "storybook": "start-storybook -p 3000",
    "build-storybook": "build-storybook"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "lint-staged": {
    "**/*.{js,jsx}": [
      "eslint ./src/**/*.{js,jsx} --fix",
      "prettier --write ./src/**/*.{js,jsx}",
      "git add"
    ]
  },
  "jest": {
    "collectCoverageFrom": [
      "src/**/*.{js,jsx}",
      "!src/**/index.{js,jsx}",
      "!src/index.js"
    ]
  },
  "devDependencies": {
    "@babel/core": "^7.11.6",
    "@babel/preset-env": "^7.11.5",
    "@babel/preset-flow": "^7.10.4",
    "@babel/preset-react": "^7.10.4",
    "@dump247/storybook-state": "^1.6.1",
    "@storybook/react": "^5.3.18",
    "babel-eslint": "^10.1.0",
    "babel-loader": "^8.1.0",
    "enzyme": "^3.10.0",
    "enzyme-adapter-react-16": "^1.14.0",
    "eslint-config-airbnb": "^17.1.1",
    "eslint-config-prettier": "^4.3.0",
    "eslint-plugin-flowtype": "^4.3.0",
    "eslint-plugin-import": "^2.18.2",
    "eslint-plugin-jsx-a11y": "^6.2.3",
    "eslint-plugin-prettier": "^3.1.0",
    "eslint-plugin-react": "^7.14.2",
    "flow-bin": "^0.107.0",
    "husky": "^2.7.0",
    "jest-canvas-mock": "^2.1.0",
    "lint-staged": "^9.2.0",
    "prettier": "^1.18.2",
    "pretty-quick": "^1.11.1",
    "webpack-cli": "^3.3.12",
    "webpack-node-externals": "^2.5.2"
  }
}
